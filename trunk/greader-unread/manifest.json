{
  "name": "Google Reader Unread Count",
  "version": "12",
  "description": "Display actual unread count instead of '1000+' in Google Reader.",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/reader/*", "https://www.google.com/reader/*",
                  "http://www.google.com.hk/reader/*", "https://www.google.com.hk/reader/*",
                  "http://www.google.co.uk/reader/*", "https://www.google.co.uk/reader/*",
                  "http://www.google.co.jp/reader/*", "https://www.google.co.jp/reader/*",
                  "http://www.google.co.kr/reader/*", "https://www.google.co.kr/reader/*",
                  "http://www.google.com.au/reader/*", "https://www.google.com.au/reader/*"],
      "js": ["googlereaderunreadcount.user.js"]
    }
  ],
  "icons": {
    "128": "icon-128.png"
  }
}